# commited env vars
# override in your own .env.local
# prefix with NEXT_PUBLIC_ to expose to front
# https://nextjs.org/docs/basic-features/environment-variables

### DO NOT EDIT THIS FILE ###
### Overload it in your own .env.local ###

PRISMA_DATABASE_URL="file:./db/sqlite.db"

# autoload internally, will fill CONFIG.HOSTNAME
NEXTAUTH_URL=http://localhost:4000
# autoload internally
NEXTAUTH_SECRET=secret

NEXT_PUBLIC_DEMO_VIDEO_ID=demo-video

# datadog disabling
FORCE_DATADOG_IN_DEV=false
# autoload internally
DD_TRACE_ENABLED=false


