{
  "name": "mojo",
  "private": true,
  "version": "1.0.0",
  "license": "private",
  "engines": {
    "node": ">=16.13.1",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "setup": "bash ./tools/scripts/setup-or-reset-local.sh",
    "dev": "source .env && source .env.local && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config=./tools/jest.config.js",
    "studio": "source .env && source .env.local && prisma studio --browser=none",
    "postinstall": "skip-postinstall || prisma generate"
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma",
    "seed": "ts-node -r tsconfig-paths/register src/server/prisma/seeds/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "md5": "^2.3.0",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@types/jest": "^27.4.0",
    "@types/md5": "^2.3.1",
    "@types/node": "16",
    "@types/react": "17.0.38",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "jest": "^27.4.7",
    "prisma": "^3.8.1",
    "skip-postinstall": "^1.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.5.5"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "./tools/.eslintrc.json"
    ]
  }
}
